    "version": "0.2.0",
    "configurations": [
        {
            "type": "dashmips",
            "request": "launch",
            "name": "Dashmips (Run With Logging)",
            "program": "${file}",
            "registerFormat": "dec",
            "cwd": "${workspaceFolder}",
            "host": "localhost",
            "port": 2390,
            "dashmipsCommand": "python -m dashmips debug",
            "dashmipsArgs": [
                "-i",
                "localhost",
                "-p",
                "2390",
                "-l"
            ],
            "args": [],
            "stopOnEntry": false
        },
        {
            "type": "dashmips",
            "request": "launch",
            "name": "Dashmips (Run)",
            "program": "${file}"
        },
        {
            "type": "dashmips",
            "request": "attach",
            "name": "Dashmips (Attach)",
            "registerFormat": "dec",
            "host": "localhost",
            "port": 2390,
            "stopOnEntry": false
        },
        {
            "command": "${workspaceFolder}/mars.sh ${file}",
            "name": "Run mips",
            "request": "launch",
            "type": "node-terminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}